{
	"name" : "gossi/php-code-generator",
	"description" : "Toolset for generating PHP code",
	"type" : "library",
	"license" : "Apache2",
	"authors" : [{
			"name" : "Thomas Gossmann",
			"homepage" : "http://gos.si"
		}
	],
	"keywords" : [
		"code generation"
	],
	"support" : {
		"issues" : "https://github.com/gossi/php-code-generator/issues"
	},
	"autoload" : {
		"psr-4" : {
			"gossi\\codegen\\" : "src/"
		}
	},
	"autoload-dev" : {
		"psr-4" : {
			"gossi\\codegen\\tests\\" : "tests/"
		}
	},
	"require" : {
		"php" : ">=5.6",
		"gossi/docblock" : "~1",
		"gossi/php-code-profiles" : "dev-master",
		"gossi/php-code-formatter" : "dev-master",
		"symfony/options-resolver" : "^4|^3",
		"phootwork/file" : "~0",
		"phootwork/tokenizer" : "~0",
		"nikic/php-parser" : "^4|^3|^2|^1"
	},
	"require-dev" : {
		"phpunit/phpunit" : "^5.7"
	},
	"suggest" : {
		"gossi/php-code-formatter" : "Formatting your generated code",
		"sami/sami" : "API Documentation"
	},
	"scripts" : {
		"test" : "phpunit",
		"api" : "sami.php update sami.php"
	}
}
